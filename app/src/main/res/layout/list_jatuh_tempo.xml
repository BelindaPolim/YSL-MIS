<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white">

    <TextView
        android:id="@+id/nomor"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:fontFamily="@font/didact_gothic"
        android:textSize="16sp"
        android:text="YSL-AU-2020-10-0128  "
        android:textColor="@color/itemuda"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@id/tanggal"
        android:padding="5dp"
        android:textAlignment="textStart"
        android:visibility="invisible"/>

    <TextView
        android:id="@+id/tanggal"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:fontFamily="@font/didact_gothic"
        android:textSize="16sp"
        android:text="20-20-2020"
        android:textColor="@color/itemuda"
        android:padding="5dp"
        android:layout_toStartOf="@id/jt"
        android:textAlignment="center"
        android:visibility="invisible"/>

    <TextView
        android:id="@+id/jt"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:fontFamily="@font/didact_gothic"
        android:textSize="16sp"
        android:text="  16  "
        android:textColor="@color/itemuda"
        android:padding="5dp"
        android:layout_toStartOf="@id/nilai"
        android:textAlignment="center"
        android:visibility="invisible"/>

    <TextView
        android:id="@+id/nilai"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:fontFamily="@font/didact_gothic"
        android:textSize="16sp"
        android:text="9.999.900.000"
        android:textColor="@color/itemuda"
        android:layout_alignParentEnd="true"
        android:padding="5dp"
        android:textAlignment="textEnd"
        android:visibility="invisible"/>

    <TextView
        android:id="@+id/jtNomor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/didact_gothic"
        android:textSize="16sp"
        android:text="YSL-AU-2020-10-0128"
        android:textColor="@color/itemuda"
        android:layout_alignParentStart="true"
        android:padding="5dp"
        android:textAlignment="textStart"
        android:layout_below="@+id/nomor"
        android:layout_alignEnd="@+id/nomor"/>

    <TextView
        android:id="@+id/jtTanggal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/didact_gothic"
        android:textSize="16sp"
        android:text="05-10-2020"
        android:textColor="@color/itemuda"
        android:padding="5dp"
        android:layout_toEndOf="@id/jtNomor"
        android:textAlignment="center"
        android:layout_below="@+id/tanggal"
        android:layout_alignStart="@id/tanggal"
        android:layout_alignEnd="@+id/tanggal"/>

    <TextView
        android:id="@+id/jtJatuhTempo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/didact_gothic"
        android:textSize="16sp"
        android:text="  16  "
        android:textColor="@color/itemuda"
        android:padding="5dp"
        android:layout_toEndOf="@id/jtTanggal"
        android:textAlignment="center"
        android:layout_below="@+id/jt"
        android:layout_alignStart="@id/jt"
        android:layout_alignEnd="@+id/jt"/>

    <TextView
        android:id="@+id/jtNilai"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/didact_gothic"
        android:textSize="16sp"
        android:text="34.200.000"
        android:textColor="@color/itemuda"
        android:layout_alignParentEnd="true"
        android:padding="5dp"
        android:layout_toEndOf="@id/jtJatuhTempo"
        android:textAlignment="textEnd"
        android:layout_below="@+id/nilai"
        android:layout_alignStart="@+id/nilai"/>

